		</method>
		<method name="addIndicatorRunningLeq">
			<arg name="name" type="s" direction="in"/>
			<arg name="channel" type="i" direction="in"/>
		</method>
		<method name="addIndicatorRecompLeq">
			<arg name="name" type="s" direction="in"/>
			<arg name="channel" type="i" direction="in"/>
		</method>
		<method name="deleteIndicatorAt">
			<arg name="index" type="i" direction="in"/>
		</method>
	</interface>
	<node name="1">
